{
  "name": "@classytic/mongoose-slug-plugin",
  "version": "1.0.1",
  "description": "A robust Mongoose plugin for automatic slug generation with duplicate handling",
  "type": "module",
  "main": "./index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./index.js"
    }
  },
  "files": [
    "index.js",
    "src/",
    "dist/types/",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "mongoose",
    "plugin",
    "slug",
    "slugify",
    "unique-slug",
    "mongodb",
    "url-slug",
    "seo",
    "permalink",
    "multi-tenant",
    "amazon-style-slug",
    "duplicate-handling"
  ],
  "author": "Sadman Chowdhury (Github: @siam923)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siam923/mongoose-slug-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/siam923/mongoose-slug-plugin/issues"
  },
  "homepage": "https://github.com/siam923/mongoose-slug-plugin#readme",
  "peerDependencies": {
    "mongoose": ">=6.0.0",
    "slugify": ">=1.6.0"
  },
  "devDependencies": {
    "@types/node": "^22.8.7",
    "mongoose": "^9.0.0",
    "slugify": "^1.6.6",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "types": "tsc -p tsconfig.types.json",
    "build": "npm run types",
    "prepublishOnly": "npm run build",
    "test:basic": "node test/test.js",
    "test:integration": "node test/integration.test.js",
    "test": "npm run test:basic && npm run test:integration"
  }
}

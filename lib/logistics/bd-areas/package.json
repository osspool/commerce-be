{
  "name": "@classytic/bd-areas",
  "version": "1.0.0",
  "description": "Bangladesh delivery areas with multi-provider support (RedX, Pathao, Steadfast) - 8 divisions, 64 districts, 2836+ areas",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "keywords": [
    "bangladesh",
    "bd",
    "areas",
    "divisions",
    "districts",
    "delivery",
    "logistics",
    "redx",
    "pathao",
    "steadfast",
    "address",
    "dropdown",
    "cascading",
    "typescript"
  ],
  "author": "Sadman Chowdhury (Github: @siam923)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/classytic/bd-logistics.git",
    "directory": "bd-areas"
  },
  "bugs": {
    "url": "https://github.com/classytic/bd-logistics/issues"
  },
  "homepage": "https://github.com/classytic/bd-logistics/tree/main/bd-areas#readme",
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "release": "npm run clean && npm run build && npm publish --access public",
    "release:dry": "npm run clean && npm run build && npm pack --dry-run"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.8.7",
    "rimraf": "^6.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  }
}

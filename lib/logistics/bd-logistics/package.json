{
  "name": "@classytic/bd-logistics",
  "version": "1.0.2",
  "description": "Production-grade Bangladesh logistics SDK - RedX, Pathao, Steadfast with unified API, circuit breaker, and retries",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./providers": {
      "types": "./dist/providers/index.d.ts",
      "import": "./dist/providers/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "bangladesh",
    "logistics",
    "redx",
    "pathao",
    "steadfast",
    "shipping",
    "delivery",
    "cod",
    "cash-on-delivery",
    "ecommerce",
    "courier",
    "parcel",
    "tracking",
    "webhook",
    "typescript",
    "circuit-breaker",
    "retry"
  ],
  "author": "Sadman Chowdhury (Github: @siam923)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/classytic/bd-logistics.git",
    "directory": "bd-logistics"
  },
  "bugs": {
    "url": "https://github.com/classytic/bd-logistics/issues"
  },
  "homepage": "https://github.com/classytic/bd-logistics/tree/main/bd-logistics#readme",
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "release": "npm run clean && npm run build && npm publish --access public",
    "release:dry": "npm run clean && npm run build && npm pack --dry-run"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.8.7",
    "rimraf": "^6.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  }
}
